<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.image.ImageView?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.computer_assembly.InstructionController"
            prefHeight="400.0" prefWidth="600.0">

    <children>
        <Button text="Назад" fx:id="startMainButton" onAction="#onBackMainClick"
                layoutX="25" layoutY="25" style="-fx-font-size: 18px; -fx-background-color: #5d5591; -fx-text-fill: white;"/>
        <ImageView fx:id="instructionImageView" fitWidth="1000.0" fitHeight="600.0"
                   layoutX="200.0" layoutY="200.0" pickOnBounds="true" preserveRatio="true"
        />
    </children>

</AnchorPane>
